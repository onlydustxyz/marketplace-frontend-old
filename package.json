{
  "name": "marketplace-frontend",
  "private": true,
  "version": "0.1.1",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build --mode staging",
    "build:prod": "tsc && vite build && rimraf dist/**/*.js.map",
    "lint": "eslint '**/*.{js,jsx,json,ts,tsx}'",
    "test": "vitest",
    "e2e": "nyc cypress run --headless",
    "e2e:open": "nyc cypress open",
    "test:coverage": "vitest --coverage",
    "preview": "vite preview",
    "postinstall": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.4",
    "@sentry/react": "^7.7.0",
    "@sentry/tracing": "^7.7.0",
    "@starknet-react/core": "0.13.4",
    "@tailwindcss/line-clamp": "^0.4.0",
    "axios": "^0.27.2",
    "bn.js": "^5.2.1",
    "classnames": "^2.3.1",
    "get-starknet": "^1.5.0",
    "github-markdown-css": "^5.1.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-error-boundary": "^3.1.4",
    "react-hot-toast": "^2.3.0",
    "react-markdown": "^8.0.3",
    "react-router-dom": "6",
    "react-use": "^17.4.0",
    "react-virtuoso": "^2.18.0",
    "recoil": "^0.7.4",
    "starknet": "^4.4.2",
    "tailwindcss-textshadow": "^2.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@commitlint/config-conventional": "^17.0.3",
    "@cypress/code-coverage": "^3.10.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@savvywombat/tailwindcss-grid-areas": "^2.1.0",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/builder-vite": "^0.2.4",
    "@storybook/react": "^6.5.13",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/react": "^13.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/bn.js": "^5.1.0",
    "@types/node": "^18.0.1",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "@vitejs/plugin-react": "^1.3.0",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "c8": "^7.12.0",
    "commitlint": "^17.0.3",
    "cypress": "10.9.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-storybook": "^0.6.6",
    "husky": "^8.0.1",
    "jsdom": "^20.0.0",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.1.2",
    "typescript": "^4.6.3",
    "vite": "^3.0.0",
    "vite-plugin-istanbul": "^3.0.1",
    "vite-plugin-sentry": "^1.1.2",
    "vitest": "^0.18.0"
  },
  "nyc": {
    "all": true,
    "cache": false,
    "extension": [
      ".js",
      ".ts",
      ".tsx"
    ],
    "include": "src/*",
    "exclude": [
      "App/tests",
      "**/*.d.ts",
      "**/*.test.ts",
      "**/*.test.tsx"
    ],
    "reporter": [
      "html",
      "clover"
    ]
  }
}
